{
    "name": "pegs",
    "version": "0.1.0",
    "main": "lib/index.js",
    "bin": {
        "pegs": "lib/_command.js"
    },
    "repository": "https://github.com/krisnye/pegs.git",
    "scripts": {
        "build": "tsc -p . & node lib/compiler/Generator.js & node lib/tests/Unit.js",
        "watch": "tsc -p . --watch & nodemon --watch lib --watch test lib/tests/Unit.js &  nodemon --watch src -e pegjs,ts,js lib/compiler/Generator.js",
        "watchWin": "start tsc -p . --watch & start nodemon --watch src -e pegjs,ts,js lib/compiler/Generator.js & nodemon --watch lib --watch test lib/tests/Unit.js",
        "buildPegJS": "pegjs -o lib/compiler/PegJsSelfParser.js src/compiler/PEGjs.pegjs",
        "perf" : "tsc -p . & node lib/tests/ParserPerformance.js",
        "profile" : "tsc -p . & node --prof lib/tests/ParserProfile.js"
    },
    "directories": {
        "lib": "lib",
        "src": "src"
    },
    "engines": {
        "node": "0.10.x"
    },
    "dependencies": {},
    "devDependencies": {
        "pegjs": "~0.10.0",
        "typescript": "^2.2.1"
    },
    "author": {
        "name": "Kris Nye",
        "email": "krisnye@gmail.com"
    },
    "contributors": [
        {
            "name": "Kody King"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/krisnye/pegs/issues"
    },
    "homepage": "https://github.com/krisnye/pegs"
}
