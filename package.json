{
    "name": "pegs",
    "version": "0.1.0",
    "main": "lib/index.js",
    "bin": {
        "pegs": "lib/_command.js"
    },
    "repository": "https://github.com/krisnye/pegs.git",
    "scripts": {
        "build": "tsc -p .",
        "watch": "tsc -p . --watch & nodemon --watch lib --watch test lib/tests/Unit.js",
        "watchWin": "start tsc -p . --watch & nodemon --watch lib --watch test lib/tests/Unit.js",
        "parserGen": "tsc -p . --watch & nodemon --watch src -e pegjs,ts,js lib/compiler/Generator.js",
        "parserGenWin": "start tsc -p . --watch & nodemon --watch src -e pegjs,ts,js lib/compiler/Generator.js",
        "parserTest": "tsc -p . & node lib/compiler/Generator.js & node lib/tests/GeneratorTest.js"
    },
    "directories": {
        "lib": "lib",
        "src": "src"
    },
    "engines": {
        "node": "0.10.x"
    },
    "dependencies": {},
    "devDependencies": {
        "pegjs": "~0.10.0",
        "typescript": "^2.2.1"
    },
    "author": {
        "name": "Kris Nye",
        "email": "krisnye@gmail.com"
    },
    "contributors": [
        {
            "name": "Kody King"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/krisnye/pegs/issues"
    },
    "homepage": "https://github.com/krisnye/pegs"
}
